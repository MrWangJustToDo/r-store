{
  "version": "0.1.0",
  "license": "MIT",
  "name": "reactive-store",
  "author": "MrWangJustToDo",
  "engines": {
    "node": ">=10"
  },
  "packageManager": "pnpm@7.5.0",
  "scripts": {
    "dev": "node ./dev.js",
    "dev:ui": "cd ui && pnpm run dev",
    "dev:packages": "ts-node ./scripts/rollupWatch.ts",
    "build:packages": "ts-node ./scripts/rollupBuild.ts",
    "lint": "eslint --cache --ext ts,tsx .",
    "lint:fix": "pnpm run lint --fix",
    "prettier": "prettier --ignore-path .prettierignore --write .",
    "clean": "pnpm -p -r -c exec rm -rf dist dev .cache",
    "purge": "pnpm -p -r -c exec rm -rf node_modules"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/runtime": "^7.21.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@swc/core": "^1.3.38",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "react": "^18.0.2",
    "rollup": "^3.18.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.7.4",
    "v-r-store": "workspaces: *"
  }
}
